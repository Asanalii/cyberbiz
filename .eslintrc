{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-alert": "warn",
    "no-console": "warn",
    "indent": ["error", 2],
    "semi": "error",
    "quotes": ["error", "single"],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "no-unused-vars": "warn"
  }
}
